;zeller.ss
(define (day-of-week)
 (zeller dd
      (cond((> 3 mm)(+ mm 10))(#t (- mm 2)))
      (remainder (cond((> 3 mm)(- yyyy 1))(#t yyyy))100)
      (quotient (cond((> 3 mm)(- yyyy 1))(#t yyyy))100)
 )
)
(define (zeller d m y c)
 (neg-to-pos (remainder (+ d y
                          (quotient (-(* 26 m)2) 10)
                          (quotient y 4)
                          (quotient c 4)
                          (* 2(- c))
                       )
            7)
 )
)
(define (neg-to-pos d)
 (cond((> 0 d)(+ d 7))
      (#t d)
 )
)
(define (birthday dw)
;                  ^{0,...,6}
 (display "Your were born on ")
    (display
     (cond((= dw 1)"Monday ")
          ((= dw 2)"Tuesday ")
          ((= dw 3)"Wednesday ")
          ((= dw 4)"Thursday ")
          ((= dw 5)"Friday ")
          ((= dw 6)"Saturday ")
          (else "Sunday ") ))
  (display dd)(display ".")
  (display mm)(display ".")
 yyyy
)
(define dd 4)
(define mm 2)
(define yyyy 2000)
(birthday (day-of-week))
